{
  "name": "naylence-agent-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "Naylence Agent SDK - TypeScript port of the Python agent orchestration toolkit",
  "author": "Naylence Dev <naylencedev@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "naylence",
    "agent",
    "sdk",
    "typescript",
    "fame",
    "messaging",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naylence/naylence-agent-sdk-ts"
  },
  "bugs": {
    "url": "https://github.com/naylence/naylence-agent-sdk-ts/issues"
  },
  "homepage": "https://github.com/naylence/naylence-agent-sdk-ts#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/esm/plugin.js",
      "require": "./dist/cjs/plugin.js",
      "types": "./dist/types/plugin.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:cjs && npm run build:esm && npm run build:browser",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:browser": "rollup -c rollup.config.js",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "tsc --watch --outDir dist/esm",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "naylence-core": "file:../naylence-core-ts",
    "naylence-factory": "file:../naylence-factory-ts",
    "naylence-runtime": "file:../naylence-runtime-ts",
    "zod": "^4.1.11"
  },
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.6.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.3",
    "ts-jest": "^29.4.4",
    "tslib": "^2.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
